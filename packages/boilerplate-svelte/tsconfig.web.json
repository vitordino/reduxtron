{
	"extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
	"include": [
		"src/shared/**/*",
		"src/preload/index.d.ts",
		"src/renderer/env.d.ts",
		"src/renderer/**/*",
		"src/renderer/**/*.svelte",
		"src/preload/**/*.ts",
	],
	"compilerOptions": {
		"composite": true,
		"baseUrl": "./",
		"skipLibCheck": true,
		"paths": {
			"src/renderer/*": ["src/renderer/*"],
			"src/*": ["src/*"]
		}
	}
}