{
	"extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
	"include": [
		"src/shared/**/*",
		"src/renderer/*.d.ts",
		"src/renderer/**/*",
		"src/renderer/**/*.svelte",
		"src/renderer/*",
		"src/renderer/*.svelte"
	],
	"compilerOptions": {
		"composite": true,
		"baseUrl": "./",
		"skipLibCheck": true,
		"moduleResolution": "Bundler",
		"paths": {
			"src/*": ["src/*"]
		}
	}
}
