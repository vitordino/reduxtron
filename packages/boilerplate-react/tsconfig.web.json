{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/shared/**/*",
    "src/renderer/env.d.ts",
    "src/renderer/**/*",
    "src/renderer/**/*.svelte",
    "src/renderer/**/*.tsx",
    "src/preload/*.d.ts"
  ],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "baseUrl": ".",
    "moduleResolution": "Bundler"
  }
}
